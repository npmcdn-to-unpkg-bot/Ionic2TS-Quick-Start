{
  "name": "Ionic2TS-Quick-Start",
  "version": "1.0.0",
  "description": "Getting started with Ionic 2 using TypeScript",
  "scripts": {
    "start": "npm run pug:index && npm run tsc:system && concurrently \"npm run tsc:w\" \"npm run lite\"",
    "pug:index": "pug src/index.pug",
    "tsc:system": "tsc src/system.config.ts",
    "tsc:w": "tsc -w src/system.config.ts",
    "lite": "lite-server -c bsconfig.json",
    "reset": "git reset --hard HEAD && git clean -df"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Ionic2TS-Quick-Start.git"
  },
  "keywords": [
    "Ionic 2",
    "TypeScript"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Ionic2TS-Quick-Start/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Ionic2TS-Quick-Start#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "core-js": "^2.4.1",
    "ionic-angular": "^2.0.0-beta.11",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.36",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "pug-cli": "^1.0.0-alpha6",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
